package exercise_3;

import java.util.Scanner;

public class P5_1d {

	public static void main(String[] args) {
		Scanner n = new Scanner(System.in);
		System.out.println("Input integer 1: ");
		int a = n.nextInt();
		System.out.println("Input integer 2: ");
		int b = n.nextInt();
		int sum = 0;
		for (int i = a; i <= b; i++){
			if(i%2 == 0){
				sum=sum+i;
			}
		}
	}

}
