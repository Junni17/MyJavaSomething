package model;

import java.util.Scanner;

public class halshe {

	public static void main(String[] args) {

		Scanner derp = new Scanner(System.in);
		System.out.println("Er Bella sød?");
		String bellaErSød = derp.nextLine();

		String yolo = "Ja";
		
		if (bellaErSød == yolo) {
			System.out.println("Ja hun er så!!!!!!!<3");
		}
		derp.close();

	}

}
