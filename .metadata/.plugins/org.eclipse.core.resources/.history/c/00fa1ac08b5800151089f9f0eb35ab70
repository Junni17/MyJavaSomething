	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		System.out.print("Input number: ");
		int a = in.nextInt();
		int sum = 0;
		while (a != 0){
			int b = a % 10;
			if (b != 0){
				sum = sum + b;
			}
			a = a / 10;
		}
		

	}

}
